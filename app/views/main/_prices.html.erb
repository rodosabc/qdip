<section id="prices">
  <div class="container">
    <div class="row">
      <div class="col-lg-12 text-center">
        <h2 class="section-heading">Наши цены</h2>
        <h3 class="section-subheading text-muted">Цены на различные типы машин.</h3>
      </div>
    </div>
    <div class="row">
      <%@price_items.each do |item|%>
      <div class="col-md-4 col-sm-6 price-item">
        <a href="#contact_us" class="price-link price-blur page-scroll">
          <div class="price-hover">
            <div class="price-hover-content">
              <%=item.price%>
            </div>
          </div>
          <%=image_tag(item.photo, :class => 'img-responsive')%>
        </a>
        <div class="price-caption">
          <h4><%=item.title%></h4>
        </div>
      </div>
      <%end%>
    </div>
  </div>
</section>