<section id="prices">
  <div class="container">
    <div class="row">
      <div class="col-lg-12 text-center">
        <h2 class="section-heading">Наши цены</h2>
        <h3 class="section-subheading text-muted">Цены на различные типы машин.</h3>
      </div>
    </div>
    <div class="row">
      <%@samples.each do |sample|%>
      <div class="col-md-4 col-sm-6 portfolio-item">
        <a href="#portfolioModal1" class="portfolio-link" data-toggle="modal">
          <div class="portfolio-hover">
            <div class="portfolio-hover-content">
              FUCK YOU
            </div>
          </div>
          <%=image_tag(sample.photo, :class => 'img-responsive')%>
        </a>
        <div class="portfolio-caption">
          <h4>Round Icons</h4>
        </div>
      </div>
      <%end%>
    </div>
  </div>
</section>